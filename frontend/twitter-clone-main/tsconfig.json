{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/SideBar/index.tsx",
    "pages/api/auth/[...nextauth].ts",
    "Firebase.ts",
    "components/Feed/DisplayTweets/index.tsx",
    "components/Feed/index.tsx",
    "pages/auth/signin.jsx",
    "Redux/features/DataSlice.js",
    "pages/[userId]/status/[tweetId].js",
    "context/DataContext.jsx",
    "components/Feed/DisplayTweets/Actions.tsx",
    "components/Feed/DisplayTweets/Like.tsx",
    "components/Feed/Icons.tsx",
    "components/Widgets/index.jsx",
    "axios.js"
  ],
  "exclude": ["node_modules"]
}
